# Azure OpenAI Configuration
# Copy this file to .env and fill in your actual values

# Your Azure OpenAI endpoint (e.g., https://your-resource.openai.azure.com/)
AZURE_OPENAI_ENDPOINT=

# Your Azure OpenAI API key
AZURE_OPENAI_API_KEY=

# Your deployment name (the name you gave your deployed model)
AZURE_OPENAI_DEPLOYMENT_NAME=

# API version (use 2024-02-15-preview or newer)
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Optional: For web search examples (get free key at tavily.com)
TAVILY_API_KEY=
